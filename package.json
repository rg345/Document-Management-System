{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "Document management system ",
  "scripts": {
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "npm --prefix client start",
    "start:server": "npm --prefix server start",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "npm --prefix client start",
    "dev:server": "npm --prefix server dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rg345/Document-Management-System.git"
  },
  "keywords": [
    "PERN",
    "Material-UI",
    "NodeJs",
    "ReactJs",
    "ExpressJs",
    "PostgreSQL"
  ],
  "author": "Rohit Gunwal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rg345/Document-Management-System/issues"
  },
  "homepage": "https://github.com/rg345/Document-Management-System#readme",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
